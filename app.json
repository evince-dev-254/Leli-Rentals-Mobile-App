      "firebaseStorageBucket": "leli-rentals-52a08.firebasestorage.app",
      "firebaseMessagingSenderId": "220739389697",
      "firebaseAppId": "1:220739389697:web:701c8d4141b29d88a13300",
      "router": {},
      "eas": {
